  </joint>


  <link name="middle">
    <visual>
       <origin rpy="1.57059633 3.1415926535 1.57059633" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://robot/models/actuatorBot/meshes/middle1.dae"/>
      </geometry>
