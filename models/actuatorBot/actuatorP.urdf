     </inertial> 
    <collision>
      <geometry>
        <box size="0.12 0.03 0.015"/>
      </geometry>
    </collision>
  </link>
     
  <joint name="base_to_carriage2" type="prismatic">
    <parent link="base_link"/>
    <child link="carriage2"/>
    <origin xyz="0 -0.045 0.0075"/>
    <axis xyz = "0 1 0"/>
